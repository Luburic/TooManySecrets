<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns:jaxws="http://cxf.apache.org/jaxws"
	xsi:schemaLocation="http://www.springframework.org/schema/beans
		http://www.springframework.org/schema/beans/spring-beans.xsd
		http://cxf.apache.org/jaxws http://cxf.apache.org/schemas/jaxws.xsd"
		>

		<jaxws:endpoint 
		id="nalog"
		address="/BankaNalog"
		implementor="provider.banka.NalogProvider"
		wsdlLocation="WEB-INF/wsdl/BankaNalog.wsdl"
		/>

		
			
		<jaxws:endpoint 
		id="izvod"
		address="/BankaIzvod"
		implementor="provider.banka.IzvodProvider"
		wsdlLocation="WEB-INF/wsdl/BankaIzvod.wsdl"
		/>
		
		<jaxws:endpoint 
		id="mt"
		address="/BankaMT"
		implementor="provider.banka.MTProvider"
		wsdlLocation="WEB-INF/wsdl/OdobrenjeMt.wsdl"
		/>
		
		
</beans>
  
