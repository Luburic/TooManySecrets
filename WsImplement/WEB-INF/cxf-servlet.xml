<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns:jaxws="http://cxf.apache.org/jaxws"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
		http://www.springframework.org/schema/beans/spring-beans.xsd
		http://cxf.apache.org/jaxws http://cxf.apache.org/schemas/jaxws.xsd">


		<jaxws:endpoint 
		id="faktura"
		address="/Faktura"
		implementor="ws.style.message.FakturaProvider"
		wsdlLocation="WEB-INF/wsdl/Firma.wsdl"
		/>


		<jaxws:endpoint 
		id="rtgs"
		address="/Rtgs"
		implementor="ws.style.message.RtgsProvider"
		wsdlLocation="WEB-INF/wsdl/Banka.wsdl"
		/>
		
		<jaxws:endpoint 
		id="clearing"
		address="/Clearing"
		implementor="ws.style.message.ClearingProvider"
		wsdlLocation="WEB-INF/wsdl/Banka.wsdl"
		/>
		
	
		<jaxws:endpoint 
		id="odobrenje"
		address="/Odobrenje"
		implementor="ws.style.message.OdobrenjeProvider"
		wsdlLocation="WEB-INF/wsdl/Banka.wsdl"
		/>
	
	
		<jaxws:endpoint 
			id="mt1023"
			address="/MT1023"
			implementor="ws.style.message.MT1023Provider"
			wsdlLocation="WEB-INF/wsdl/Banka.wsdl"
			/>
			
		<jaxws:endpoint 
		id="izvod"
		address="/Izvod"
		implementor="ws.style.message.IzvodProvider"
		wsdlLocation="WEB-INF/wsdl/Banka.wsdl"
		/>
		
</beans>
  
