<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns:jaxws="http://cxf.apache.org/jaxws"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
		http://www.springframework.org/schema/beans/spring-beans.xsd
		http://cxf.apache.org/jaxws http://cxf.apache.org/schemas/jaxws.xsd">


		<jaxws:endpoint 
		id="faktura"
		address="/Faktura"
		implementor="ws.style.message.FakturaProvider"
		wsdlLocation="WEB-INF/wsdl/Firma.wsdl"
		/>


		<jaxws:endpoint 
		id="nalog"
		address="/Nalog"
		implementor="ws.style.message.NalogProvider"
		wsdlLocation="WEB-INF/wsdl/Banka.wsdl"
		/>
		
	
		<jaxws:endpoint 
		id="izvod"
		address="/Izvod"
		implementor="ws.style.message.IzvodProvider"
		wsdlLocation="WEB-INF/wsdl/Banka.wsdl"
		/>
		
</beans>
  
