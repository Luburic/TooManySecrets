<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns="http://www.toomanysecrets.com/tipovi"
    targetNamespace="http://www.toomanysecrets.com/tipovi"
    xmlns:xenc="http://www.w3.org/2001/04/xmlenc#" elementFormDefault="qualified">
    
    <xs:import namespace="http://www.w3.org/2001/04/xmlenc#" schemaLocation="xenc-schema.xsd"/>
    <xs:element name="notification">
        <xs:complexType>
            <xs:sequence>
                <xs:element ref="xenc:EncryptedData"/>
            </xs:sequence>
        </xs:complexType>
    </xs:element>
</xs:schema>
